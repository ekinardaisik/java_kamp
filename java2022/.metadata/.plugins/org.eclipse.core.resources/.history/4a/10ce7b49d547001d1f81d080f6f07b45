package miniodevsil;

public class miniodevsil {

	public static void main(String[] args) {
		char harf = 'A' ;
		
		char[] kalınharfler = {"A","I","I","U"};
        
		if(harf == kalınharfler) {
			System.out.println("kalın harf");
			
		}
		else {
			System.out.println("ince harfler");
		}

	}

}
